  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "Countergank"
  ],
  "dependencies": {
    "@nestjs/axios": "1.0.0",
    "@nestjs/common": "9.2.1",
    "@nestjs/core": "9.2.1",
    "express-query-boolean": "2.0.0",
    "helmet": "6.0.1",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.2.1",
    "passport-jwt": "4.0.0",
    "config": "3.3.8",
    "@nestjs/swagger": "6.1.4"
