  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.1",
    "@types/config": "3.3.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.13",
    "@types/supertest": "2.0.12",
    "jest": "29.3.1",
    "nodemon": "2.0.20",
    "prettier": "2.8.1",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "tslint": "5.20.1",
    "typescript": "4.9.4",
    "@types/passport": "1.0.11",
    "@types/passport-jwt": "3.0.7"
  },
  "publishConfig": {
    "@countergank:registry": "https://npm.pkg.github.com"
